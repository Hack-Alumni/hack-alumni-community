{
  "version": 2,
  "buildCommand": "yarn vercel-build",
  "installCommand": "yarn install",
  "framework": null,
  "functions": {
    "apps/member-profile/build/server/index.js": {
      "maxDuration": 30
    },
    "apps/admin-dashboard/build/server/index.js": {
      "maxDuration": 30
    },
    "apps/api/src/main.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/apps/api/src/main.ts"
    },
    {
      "source": "/admin/(.*)",
      "destination": "/apps/admin-dashboard/build/server/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/apps/member-profile/build/server/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
